#!/bin/bash

TEST_NAME="CIS-1.1.4"
TEST_SUMMARY="Ensure nosuid option set on /tmp partition"
TEST_SEVERITY="High"

#--------
# test scripting
#--------

TEST="`mount | grep -w /tmp |grep nosuid`"
if [ -n "$TEST" ]
then
    :
else
    logit2 severity=$TEST_SEVERITY name="$TEST_NAME" summary="$TEST_SUMMARY"
fi

